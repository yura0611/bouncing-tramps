    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.0.2",
    "postcss-scss": "^4.0.2",
    "prettier": "2.4.1",
    "prettier-eslint": "^13.0.0",
