{
  "name": "tc-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration local",
    "lint": "ng lint && npm run lint:styles",
    "postinstall": "husky install",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
